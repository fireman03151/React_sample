    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "@babel/register": "^7.0.0-rc.2",
    "adler32": "~0.1.7",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "commitizen": "^2.9.6",
    "cz-freecodecamp": "^1.0.1",
    "eslint": "^4.10.0",
    "eslint-config-freecodecamp": "^1.1.1",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "joi": "^13.1.2",
    "joi-objectid": "^2.0.0",
    "loopback-component-explorer": "^2.1.1",
    "nodemon": "^1.18.3",
    "pm2": "^3.0.3",
    "prettier": "^1.14.2",
    "sinon": "^2.0.0",
    "tap-difflet": "^0.7.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "validate-commit-msg": "^2.12.2"
  },
  "snyk": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-freecodecamp"
    }
  }
