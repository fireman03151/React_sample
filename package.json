    "@freecodecamp/react-notification": "^1.0.0",
    "@freecodecamp/store": "^1.0.0",
    "accepts": "^1.3.0",
    "auth0-js": "^9.5.1",
    "axios": "^0.18.0",
    "body-parser": "^1.13.2",
    "cal-heatmap": "~3.5.2",
    "chai": "~3.4.1",
    "compression": "^1.6.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.0",
    "cors": "^2.8.4",
    "csurf": "^1.8.3",
    "d3": "~3.5.17",
    "date-fns": "^1.29.0",
    "debug": "^2.2.0",
    "dedent": "~0.7.0",
    "dotenv": "^4.0.0",
    "express-flash": "~0.0.2",
    "express-session": "^1.12.1",
