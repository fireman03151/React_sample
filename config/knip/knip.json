{
  "client":{
    "exclude": ["dependencies"],
    "entryFiles": [
      "gatsby-*.js",
      "src/html.tsx",
      "src/pages/*.tsx",
      "src/templates/Challenges/**/{s,S}how.tsx",
      "src/templates/Introduction/**/{intro,super-block-intro}.tsx",
      "**/*.test.{js,ts,tsx}"
    ],
    "projectFiles": [
      "**/*.{js,ts,tsx}",
      "!static/misc/*.js",
      "!i18n/schema-validation.ts",
      "!plugins/**",
      "!postcss.config.js",
      "!src/client/frame-runner.ts",
      "!src/client/workers/**",
      "!webpack-workers.js",
      "!**/__mocks__/**",
      "!**/__fixtures/**",
      "!**/node_modules/**",
      "!**/*.d.ts"
    ]
  }
}
